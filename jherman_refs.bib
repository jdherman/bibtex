  ISSN                     = {0022-1694},
  Keywords                 = {Climate change scenario, Flood risk, impact assessment, {IPCC}-{AR}4, {UKCIP}9, Water policy},
  Owner                    = {jdh366},
  Shorttitle               = {Scenario-neutral approach to climate change impact studies},
