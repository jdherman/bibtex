@Article{ge_combining_2014,
  Title                    = {Combining screening and metamodel-based methods: an efficient sequential approach for the sensitivity analysis of model outputs},
  Author                   = {Ge, Qiao and Ciuffo, Biagio and Menendez, Monica},
  Journal                  = {Reliability Engineering \& System Safety},
  Year                     = {2014},

  Abstract                 = {Sensitivity Analysis ({SA}) is able to identify the most influential parameters of a given model. Application of {SA} is usually critical for reducing the complexity in the subsequent model calibration and use. Unfortunately it is hardly applied, especially when the model is in the form of a computationally expensive black-box computer program. A possible solution concerns applying {SA} to the metamodel (i.e., an approximation of the computationally expensive model) instead. Among the other options, the use of Gaussian process metamodels (also known as Kriging metamodels) has been recently proposed for the {SA} of computationally expensive traffic simulation models. However, the main limitation of this approach is its dependence on the model dimensionality. When the model is high-dimensional, the estimation of the Kriging metamodel may still be problematic due to its high computational cost.

In order to overcome this problem, in the present paper, the Kriging-based approach has been combined with the quasi-Optimized Trajectory based Elementary Effects (quasi-{OTEE}) approach for the {SA} of high-dimensional models. The quasi-{OTEE} {SA} is used first to screen the influential and non-influential parameters of a high-dimensional model; then the Kriging-based {SA} is used to calculate the variance-based sensitivity indexes, and to rank the most influential parameters in a more accurate way. The application of the proposed sequential {SA} is illustrated with several numerical experiments. Results show that the method can properly identify the most influential parameters and their ranks, while the number of model evaluations is considerably less than the variance-based {SA} (e.g., in one of the tests the sequential {SA} requires over 50 times less model evaluations than the variance-based {SA}).},
  Doi                      = {10.1016/j.ress.2014.08.009},
  File                     = {ScienceDirect Full Text PDF:C\:\\Users\\jdh366\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18zv1xho.default\\zotero\\storage\\JKQG25SJ\\Ge et al. - Combining screening and metamodel-based methods a.pdf:application/pdf},
  ISSN                     = {0951-8320},
  Keywords                 = {High-dimensional and computationally expensive model, Metamodel, Screening, Sensitivity analysis, Variance-based approach},
  Owner                    = {jdh366},
  Shorttitle               = {Combining screening and metamodel-based methods},
  Timestamp                = {2014.09.09},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0951832014002026},
  Urldate                  = {2014-09-04}
}

