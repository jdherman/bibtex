  ISSN                     = {0022-1694},
  Keywords                 = {Distributed hydrologic modeling, Hydrologic simulation, Model intercomparison, Radar precipitation, Rainfall--runoff},
  Shorttitle               = {The Distributed Model Intercomparison Project ({DMIP)}},
  Urldate                  = {2013-02-27}
}

