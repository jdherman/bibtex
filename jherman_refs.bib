  ISSN                     = {0048-9697},
  Keywords                 = {Aire, Calder, {FAST}, Modelling, Ouse, {QUESTOR}, River, Rural, Sensitivity analysis, Urban, Water quality},
  Shorttitle               = {Urban Environmental Research in the {UK:} The Urban Regeneration and the Environment ({NERC} {URGENT)} Programme and associated studies.},
  Urldate                  = {2013-01-28}
}

