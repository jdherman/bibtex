  Doi                      = {10.1038/nature08823},
  File                     = {Full Text PDF:C\:\\Users\\jdh366\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\18zv1xho.default\\zotero\\storage\\KPUQERXF\\Moss et al. - 2010 - The next generation of scenarios for climate chang.pdf:application/pdf},
