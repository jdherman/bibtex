  ISSN                     = {1944-7973},
  Keywords                 = {1817 Extreme events, 1819 Geographic Information Systems ({GIS}), 1829 Groundwater hydrology, 1847 Modeling, extremes, Groundwater recharge, hydrochemistry, karst hydrology, modeling, water resources},
  Language                 = {en},
  Owner                    = {jdh366},
  Timestamp                = {2014.07.30},
  Url                      = {http://onlinelibrary.wiley.com/doi/10.1002/2014WR015685/abstract},
