  ISSN                     = {1944-7973},
  Keywords                 = {1807 Climate impacts, 1880 Water management, 1884 Water supply, 6309 Decision making under uncertainty, frequency of water shortage, London water supply, probabilistic climate projections, risk-based water resources planning},
  Language                 = {en},
